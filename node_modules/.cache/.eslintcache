setSelectedOption(prevState => {\n            return {\n                ...prevState, \n                selectedAnswer: event.target.innerText\n            }\n        })\n    }\n\n    let listOfOptions\n    if (props.submitStatus) {\n        listOfOptions = options.map((item, index) => {\n            if (selectedOption.selectedAnswer === selectedOption.rightAnswer && selectedOption.selectedAnswer === item) {\n                return (\n                    <button key={index} style={{backgroundColor: '#94D7A2', border: 'none'}}>{item}</button>\n                )\n            } else if (selectedOption.selectedAnswer !== selectedOption.rightAnswer && selectedOption.selectedAnswer === item) {\n                return (\n                    <button key={index} style={{backgroundColor: '#F8BCBC', border: 'none', color: '#7c82a4'}}>{item}</button>\n                )\n            } else if (item === selectedOption.rightAnswer) {\n                return (\n                    <button key={index} style={{backgroundColor: '#94D7A2', border: 'none'}}>{item}</button>\n                )\n            } else {\n                return (\n                    <button key={index}>{item}</button>\n                )\n            }\n        })\n    } else {\n        listOfOptions = options.map((item, index) => {\n            if (selectedOption.selectedAnswer === item) {\n                return (\n                    <button style={{backgroundColor: 'lightblue'}} onClick={select} key={index}>{item}</button>\n                )\n            } else {\n                return <button onClick={select} key={index}>{item}</button>\n            }\n            \n    })\n    }    \n\n    React.useEffect(() => {\n        if (selectedOption.rightAnswer === selectedOption.selectedAnswer) {\n            props.getCountProp(prevState => prevState + 1);\n        }\n    }, [selectedOption])\n    \n\n    return (\n        <div className=\"question\">\n            <h2>{props.question}</h2>\n            <div className=\"option\">{listOfOptions}</div>\n        </div>\n    )\n}\n\nexport default Question;",{"ruleId":"40","severity":1,"message":"41","line":9,"column":13,"nodeType":"42","endLine":9,"endColumn":54},{"ruleId":"40","severity":1,"message":"41","line":15,"column":13,"nodeType":"42","endLine":15,"endColumn":60},{"ruleId":"43","severity":1,"message":"44","line":20,"column":8,"nodeType":"45","endLine":20,"endColumn":10,"suggestions":"46"},{"ruleId":"43","severity":1,"message":"47","line":81,"column":8,"nodeType":"45","endLine":81,"endColumn":24,"suggestions":"48"},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","react-hooks/exhaustive-deps","React Hook React.useEffect has a missing dependency: 'arrayOfOptions'. Either include it or remove the dependency array.","ArrayExpression",["49"],"React Hook React.useEffect has a missing dependency: 'props'. Either include it or remove the dependency array. However, 'props' will change when *any* prop changes, so the preferred fix is to destructure the 'props' object outside of the useEffect call and refer to those specific props inside React.useEffect.",["50"],{"desc":"51","fix":"52"},{"desc":"53","fix":"54"},"Update the dependencies array to be: [arrayOfOptions]",{"range":"55","text":"56"},"Update the dependencies array to be: [props, selectedOption]",{"range":"57","text":"58"},[522,524],"[arrayOfOptions]",[2729,2745],"[props, selectedOption]"]